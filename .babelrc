{
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "components": "./components",
          "modules": "./modules",
          "pages": "./pages",
          "layout": "./layout",
          "css": "./css",
          "utils": "./utils",
          "constants": "./constants",
          "mocks": "./mocks"
        },
        "cwd": "babelrc"
      }
    ]
  ],
  "presets": [
    [
      "next/babel",
      {
        "preset-env": { "modules": "commonjs" }
      }
    ]
  ],
  "ignore": []
}
